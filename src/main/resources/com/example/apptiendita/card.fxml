<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="160.0" prefWidth="265.0" styleClass="card" stylesheets="@../../../css/ViewProducts.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="160.0" prefWidth="265.0">
         <children>
            <Pane prefHeight="16.0" prefWidth="250.0" />
            <HBox prefHeight="145.0" prefWidth="250.0">
               <children>
                  <Pane prefHeight="195.0" prefWidth="25.0" />
                  <ImageView fx:id="image" fitHeight="101.0" fitWidth="147.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../img/Default.png" />
                     </image></ImageView>
                  <VBox prefHeight="145.0" prefWidth="103.0">
                     <children>
                        <Label fx:id="name" text="Label">
                           <font>
                              <Font size="11.0" />
                           </font>
                        </Label>
                        <Pane prefHeight="9.0" prefWidth="136.0" />
                        <Label fx:id="descrip" text="Label">
                           <font>
                              <Font size="10.0" />
                           </font></Label>
                        <Pane prefHeight="27.0" prefWidth="136.0" />
                        <Label fx:id="price" text="Label">
                           <font>
                              <Font size="11.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="10.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                     <padding>
                        <Insets left="10.0" right="5.0" />
                     </padding>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
