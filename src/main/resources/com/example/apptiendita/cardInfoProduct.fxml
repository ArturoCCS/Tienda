<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>

<HBox spacing="5" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="category" alignment="CENTER" prefWidth="175.0">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
         <padding>
            <Insets bottom="3.0" left="7.0" right="7.0" top="3.0" />
         </padding>
         <font>
            <Font size="13.0" />
         </font></Label>
        <Label fx:id="price" prefWidth="100.0">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
         <padding>
            <Insets bottom="1.0" left="3.0" right="1.0" top="1.0" />
         </padding></Label>
      <Region HBox.hgrow="ALWAYS" />
    </children>
   <padding>
      <Insets bottom="5.0" top="10.0" />
   </padding>
</HBox>
